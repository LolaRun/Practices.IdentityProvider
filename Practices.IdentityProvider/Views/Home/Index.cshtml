@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using System.Security.Claims
<div class="row">
    <table>
        @foreach (var claim in ((ClaimsIdentity)User.Identity).Claims) {
            <tr>
                <td>@claim.Type</td>
                <td> @claim.Value</td>
            </tr>
        }
    </table>
</div>